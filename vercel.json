{
  "rewrites": [
    { "source": "/api/gemini_audio", "destination": "/api/gemini_audio" }
  ],
  "env": {
    "GOOGLE_API_KEY": "${GOOGLE_API_KEY}"
  },
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@3.1.0"
    }
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  }
}
