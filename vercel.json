{
  "version": 2,
  "functions": {
    "api/gemini_audio/**/*.py": {
      "runtime": "python3.11",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/api/gemini_audio", "destination": "/api/gemini_audio" }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  }
}
