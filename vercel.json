{
  "version": 2,
  "builds": [
    {
      "src": "api/gemini/**/*.py",
      "use": "@vercel/python@3.1.0",
      "config": {
        "runtime": "python3.12"
      }
    }
  ],
  "functions": {
    "api/gemini/stream.py": {
      "memory": 1024,
      "maxDuration": 60,
      "runtime": "@vercel/python@3.1.0",
      "fluidCompute": true
    },
    "api/gemini/audio.py": {
      "memory": 1024,
      "maxDuration": 30,
      "runtime": "@vercel/python@3.1.0",
      "fluidCompute": true
    }
  },
  "routes": [
    { "src": "/api/gemini/stream", "dest": "/api/gemini/stream.py" },
    { "src": "/api/gemini/audio", "dest": "/api/gemini/audio.py" }
  ],
  "env": {
    "GOOGLE_API_KEY": "${GOOGLE_API_KEY}"
  }
}
