{
  "version": 2,
  "functions": {
    "api/gemini_audio.py": {
      "runtime": "python3.11",
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/gemini_audio",
      "dest": "/api/gemini_audio.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key"
  }
}
