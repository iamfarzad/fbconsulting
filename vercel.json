{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "framework": "vite",
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@3.1.0",
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/gemini/audio",
      "dest": "/api/gemini/audio.py",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Content-Type": "audio/wav" 
      }
    },
    {
      "src": "/api/gemini/stream",
      "dest": "/api/gemini/stream.py",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" }
      ]
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "AIzaSyC77mmHQCmDsUSk86pP4IbGwtSjL-fa9qc",
    "GEMINI_API_KEY": "AIzaSyC77mmHQCmDsUSk86pP4IbGwtSjL-fa9qc"
  }
}
