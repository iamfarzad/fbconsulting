
{
  "version": 2,
  "functions": {
    "api/gemini/stream.py": {
      "memory": 1024,
      "maxDuration": 60,
      "runtime": "python3.12"
    },
    "api/gemini/audio.py": {
      "memory": 1024,
      "maxDuration": 30,
      "runtime": "python3.12"
    }
  },
  "routes": [
    { "src": "/api/gemini/stream", "dest": "/api/gemini/stream.py" },
    { "src": "/api/gemini/audio", "dest": "/api/gemini/audio.py" }
  ],
  "env": {
    "GOOGLE_API_KEY": "${GOOGLE_API_KEY}"
  }
}